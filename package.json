{
  "name": "media-downloader",
  "version": "1.0.0",
  "description": "media downloader",
  "main": "index.js",
  "scripts": {
    "_build": "webpack",
    "build:dev": "cross-env NODE_ENV=development npm run _build",
    "build:prod": "cross-env NODE_ENV=production npm run _build",
    "build": "npm run build:dev",
    "start": "pm2 start ./app.json",
    "stop": "pm2 stop ./app.json",
    "fetch": "node ./dist/fetch.js",
    "download": "node ./dist/download.js",
    "backup": "npm run fetch && npm run download"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphiredev/media-downloader.git"
  },
  "author": "sapphires",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphiredev/media-downloader/issues"
  },
  "homepage": "https://github.com/sapphiredev/media-downloader#readme",
  "dependencies": {
    "knex": "^0.16.5",
    "node-fetch": "^2.5.0",
    "puppeteer": "^1.15.0",
    "qs": "^6.7.0",
    "request": "^2.88.0",
    "sqlite3": "^4.0.6",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.3.3",
    "@types/puppeteer": "^1.12.4",
    "@types/qs": "^6.5.3",
    "@types/request": "^2.48.1",
    "@types/twit": "^2.2.21",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  }
}
