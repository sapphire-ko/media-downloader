{
  "name": "media-downloader",
  "version": "1.0.0",
  "description": "media downloader",
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rm -fr ./dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "prestart": "npm run build",
    "start": "pm2 start ./app.json",
    "stop": "pm2 stop ./app.json",
    "lint": "tslint -p ./tsconfig.json",
    "start:dev": "ts-node ./src/index.ts",
    "fetch": "ts-node ./src/fetch.ts",
    "download": "ts-node ./src/download.ts",
    "backup": "npm run fetch && npm run download"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphiredev/media-downloader.git"
  },
  "author": "sapphire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphiredev/media-downloader/issues"
  },
  "homepage": "https://github.com/sapphiredev/media-downloader#readme",
  "dependencies": {
    "knex": "^0.21.5",
    "node-fetch": "^2.6.0",
    "puppeteer": "^5.2.1",
    "qs": "^6.9.4",
    "request": "^2.88.2",
    "sqlite3": "^5.0.0",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/puppeteer": "^3.0.1",
    "@types/qs": "^6.9.4",
    "@types/request": "^2.48.5",
    "@types/twit": "^2.2.27",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
